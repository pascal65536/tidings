u043e\u043d\u0447\u0438\u043b\u0430 &ndash; \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0443\u044e \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u0443\u044e \u0430\u043a\u0430\u0434\u0435\u043c\u0438\u044e \u0446\u0432\u0435\u0442\u043d\u044b\u0445 \u043c\u0435\u0442\u0430\u043b\u043b\u043e\u0432 \u0438 \u0437\u043e\u043b\u043e\u0442\u0430, \u044d\u043a\u043e\u043d\u043e\u043c\u0438\u0441\u0442-\u043c\u0435\u043d\u0435\u0434\u0436\u0435\u0440.<\/p>\n<p>1986 &ndash; 1992&nbsp;\u2015&nbsp;\u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0438\u0439 \u043f\u0440\u043e\u043c\u0441\u0442\u0440\u043e\u0439\u043d\u0438\u0438\u043f\u0440\u043e\u0435\u043a\u0442, \u0438\u043d\u0436\u0435\u043d\u0435\u0440 \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f;<\/p>\n<p>1992 &ndash; 1993 \u2015&nbsp;\u0424\u0438\u043b\u0438\u0430\u043b \u0413\u041f\u0418 \u0438 \u041d\u0418\u0418 \u0413\u0410 \u0410\u044d\u0440\u043e\u043f\u0440\u043e\u0435\u043a\u0442 &laquo;\u0421\u0438\u0431\u0430\u044d\u0440\u043e\u043f\u0440\u043e\u0435\u043a\u0442&raquo;, \u0438\u043d\u0436\u0435\u043d\u0435\u0440;&nbsp;<\/p>\n<p>1993 &ndash; 1997 \u2015&nbsp;\u0410\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u044f \u0421\u0432\u0435\u0440\u0434\u043b\u043e\u0432\u0441\u043a\u043e\u0433\u043e \u0440\u0430\u0439\u043e\u043d\u0430 \u0432 \u0433\u043e\u0440\u043e\u0434\u0435 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0435, \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0441\u0442;<\/p>\n<p>1997 &ndash; 2008 \u2015&nbsp;\u0410\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u044f \u0433\u043e\u0440\u043e\u0434\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0430, \u0434\u0435\u043f\u0430\u0440\u0442\u0430\u043c\u0435\u043d\u0442 \u0413\u043b\u0430\u0432\u044b \u0433\u043e\u0440\u043e\u0434\u0430, \u043f\u043e\u043c\u043e\u0449\u043d\u0438\u043a \u0413\u043b\u0430\u0432\u044b \u0433\u043e\u0440\u043e\u0434\u0430;<\/p>\n<p>2008 - \u043c\u0430\u0440\u0442 2016 \u2015 \u0420\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c \u0433\u043b\u0430\u0432\u043d\u043e\u0433\u043e \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u0441\u043e\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u0437\u0430\u0449\u0438\u0442\u044b \u043d\u0430\u0441\u0435\u043b\u0435\u043d\u0438\u044f \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u0433\u043e\u0440\u043e\u0434\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0430.<\/p>\n<p>\u041c\u0430\u0440\u0442 2016&nbsp;\u2015&nbsp;\u041d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u043f\u043e \u0440\u0430\u0431\u043e\u0442\u0435 \u0441 \u043e\u0431\u0440\u0430\u0449\u0435\u043d\u0438\u044f\u043c\u0438 \u0433\u0440\u0430\u0436\u0434\u0430\u043d &ndash; \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u043f\u0440\u0438\u0435\u043c\u043d\u043e\u0439 \u0413\u0443\u0431\u0435\u0440\u043d\u0430\u0442\u043e\u0440\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f.<\/p>\n<p>\u041d\u0430\u0433\u0440\u0430\u0436\u0434\u0435\u043d\u0430 \u041f\u043e\u0447\u0435\u0442\u043d\u043e\u0439 \u0433\u0440\u0430\u043c\u043e\u0442\u043e\u0439 \u0413\u043b\u0430\u0432\u044b \u0433\u043e\u0440\u043e\u0434\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0430, \u0437\u0430 \u043c\u043d\u043e\u0433\u043e\u043b\u0435\u0442\u043d\u0438\u0439 \u0434\u043e\u0431\u0440\u043e\u0441\u043e\u0432\u0435\u0441\u0442\u043d\u044b\u0439 \u0442\u0440\u0443\u0434, \u041f\u0430\u043c\u044f\u0442\u043d\u044b\u043c \u0437\u043d\u0430\u043a\u043e\u043c &laquo;\u0417\u0430 \u0441\u043b\u0443\u0436\u0435\u043d\u0438\u0435 \u043d\u0430 \u0431\u043b\u0430\u0433\u043e \u0433\u043e\u0440\u043e\u0434\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0430&raquo;, \u0437\u0430 \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0435 \u0443\u0441\u043f\u0435\u0445\u0438 \u0438 \u0434\u043e\u0441\u0442\u0438\u0436\u0435\u043d\u0438\u044f, \u043f\u043b\u043e\u0434\u043e\u0442\u0432\u043e\u0440\u043d\u044b\u0439 \u0442\u0440\u0443\u0434 \u043d\u0430 \u0431\u043b\u0430\u0433\u043e \u0433\u043e\u0440\u043e\u0434\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0430, \u041d\u0430\u0433\u0440\u0443\u0434\u043d\u044b\u043c \u0437\u043d\u0430\u043a\u043e\u043c &laquo;\u0413\u0435\u0440\u0431 \u0433\u043e\u0440\u043e\u0434\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0430&raquo;, \u0437\u0430 \u0432\u044b\u0434\u0430\u044e\u0449\u0438\u0439\u0441\u044f \u0432\u043a\u043b\u0430\u0434 \u0432 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u0435 \u0433\u043e\u0440\u043e\u0434\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0430, \u043c\u0435\u0434\u0430\u043b\u044c\u044e \u0424\u0435\u0434\u0435\u0440\u0430\u043b\u044c\u043d\u043e\u0439 \u0441\u043b\u0443\u0436\u0431\u044b \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u043a\u0438 \"\u0417\u0430 \u0437\u0430\u0441\u043b\u0443\u0433\u0438 \u0432 \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u0438\u0438 \u0412\u0441\u0435\u0440\u043e\u0441\u0441\u0438\u0439\u0441\u043a\u043e\u0439 \u043f\u0435\u0440\u0435\u043f\u0438\u0441\u0438 \u043d\u0430\u0441\u0435\u043b\u0435\u043d\u0438\u044f 2010 \u0433\u043e\u0434\u0430\", 2012.<\/p>\n<p>\u0417\u0430\u043c\u0443\u0436\u0435\u043c, \u0435\u0441\u0442\u044c \u0441\u044b\u043d<\/p>","person_bio_short":"<p>\u041d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u043f\u043e \u0440\u0430\u0431\u043e\u0442\u0435 \u0441 \u043e\u0431\u0440\u0430\u0449\u0435\u043d\u0438\u044f\u043c\u0438 \u0433\u0440\u0430\u0436\u0434\u0430\u043d &ndash; \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u043f\u0440\u0438\u0435\u043c\u043d\u043e\u0439 \u0413\u0443\u0431\u0435\u0440\u043d\u0430\u0442\u043e\u0440\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>","person_foto":"\/static\/images\/person\/person_foto_91.jpg","person_clean_url":"bobrova-natalya-leonidovna","person_order":"0"}, {"person_id":"92","person_name":"\u041e\u043b\u044c\u0433\u0430 \u0412\u043b\u0430\u0434\u0438\u043c\u0438\u0440\u043e\u0432\u043d\u0430","person_last_name":"\u0420\u0443\u0445\u0443\u043b\u043b\u0430\u0435\u0432\u0430","person_status":"\u041c\u0438\u043d\u0438\u0441\u0442\u0440 \u0438\u043d\u0432\u0435\u0441\u0442\u0438\u0446\u0438\u0439 \u0438 \u0438\u043d\u043d\u043e\u0432\u0430\u0446\u0438\u0439 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f","person_bio":"<p>\u0414\u0430\u0442\u0430 \u0438 \u043c\u0435\u0441\u0442\u043e \u0440\u043e\u0436\u0434\u0435\u043d\u0438\u044f: 18 \u0441\u0435\u043d\u0442\u044f\u0431\u0440\u044f 1976 \u0433\u043e\u0434\u0430, \u0441\u0435\u043b\u043e \u0427\u0435\u0440\u043d\u043e\u0435 \u041e\u0437\u0435\u0440\u043e \u0428\u0438\u0440\u0438\u043d\u0441\u043a\u043e\u0433\u043e \u0440\u0430\u0439\u043e\u043d\u0430 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438 \u0425\u0430\u043a\u0430\u0441\u0438\u044f.<\/p>\n<p>\u0412 1998 \u0433\u043e\u0434\u0443 \u043e\u043a\u043e\u043d\u0447\u0438\u043b\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0438\u0439 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0430\u0433\u0440\u0430\u0440\u043d\u044b\u0439 \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442, \u0438\u043d\u0436\u0435\u043d\u0435\u0440-\u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433 \u043f\u043e \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \"\u0422\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u044f \u0445\u043b\u0435\u0431\u0430, \u043c\u0443\u0447\u043d\u044b\u0445 \u043a\u043e\u043d\u0434\u0438\u0442\u0435\u0440\u0441\u043a\u0438\u0445 \u0438 \u043c\u0430\u043a\u0430\u0440\u043e\u043d\u043d\u044b\u0445 \u0438\u0437\u0434\u0435\u043b\u0438\u0439\".<\/p>\n<p>1994 \u0433\u043e\u0434 - \u0434\u0438\u0441\u043f\u0435\u0442\u0447\u0435\u0440 \u041e\u0410\u041e &laquo;\u0423\u0447\u0443\u043c\u0441\u043a\u0438\u0439 \u043f\u043b\u0435\u043c\u0437\u0430\u0432\u043e\u0434&raquo; \u0423\u0436\u0443\u0440\u0441\u043a\u0438\u0439 \u0440\u0430\u0439\u043e\u043d<\/p>\n<p>1997 &ndash; 1998 \u0433\u043e\u0434 - \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440 \u043c\u0430\u0433\u0430\u0437\u0438\u043d\u0430 &laquo;\u0418\u043d\u0442\u0435\u0440\u044c\u0435\u0440&raquo; \u041e\u041e\u041e &laquo;\u0421\u0435\u043b\u0435\u043a\u0442&raquo;, \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a<\/p>\n<p>1998 \u0433\u043e\u0434 - \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440 \u041f\u042d\u0412\u041c \u041e\u041e\u041e &laquo;\u0414\u043e\u043c\u043e\u0432\u043e\u0439&raquo;, \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a<\/p>\n<p>1998- 1999 \u0433\u043e\u0434\u044b - \u043a\u043e\u043c\u043c\u0435\u0440\u0447\u0435\u0441\u043a\u0438\u0439 \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440 \u041e\u041e\u041e &laquo;\u041f\u0435\u043d\u0430&raquo;, \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a<\/p>\n<p>1999 \u0433\u043e\u0434 - \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a \u043e\u0442\u0434\u0435\u043b\u0430 \u043c\u0430\u0440\u043a\u0435\u0442\u0438\u043d\u0433\u0430 \u0417\u0410\u041e &laquo;\u0411\u043e\u0433\u0443\u0440\u044c&raquo;, \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a<\/p>\n<p>1999 &ndash; 2002 \u0433\u043e\u0434\u044b - \u0437\u0430\u0432\u0435\u0434\u0443\u044e\u0449\u0430\u044f \u043e\u0442\u0434\u0435\u043b\u043e\u043c \u043f\u043e \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u044e \u043f\u0440\u0435\u0434\u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u043e\u0439 \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438, \u043f\u0440\u043e\u0434\u043e\u0432\u043e\u043b\u044c\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0445 \u0440\u0435\u0441\u0443\u0440\u0441\u043e\u0432 \u0438 \u043c\u0430\u0440\u043a\u0435\u0442\u0438\u043d\u0433\u0430 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u041a\u0430\u043d\u0441\u043a\u0430<\/p>\n<p>2002 \u0433\u043e\u0434 - \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u043e\u0439 \u043f\u043e\u043b\u0438\u0442\u0438\u043a\u0438 \u0438 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u044f \u043f\u0440\u0435\u0434\u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u041a\u0430\u043d\u0441\u043a\u0430<\/p>\n<p>2002 \u0433\u043e\u0434 - \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440\u0430 \u043f\u043e \u043c\u0430\u0440\u043a\u0435\u0442\u0438\u043d\u0433\u0443 \u041e\u041e\u041e &laquo;\u0421\u0442\u0430\u043d\u0434\u0430\u0440\u0442 &ndash; \u043f\u043b\u044e\u0441&raquo;, \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a<\/p>\n<p>2002 - 2003 \u0433\u043e\u0434\u044b - \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440 \u041e\u041e\u041e &laquo;\u0424\u043e\u0440\u0435\u0441\u0442&raquo;, \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a<\/p>\n<p>2003-2004 \u0433\u043e\u0434\u044b - . \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440\u0430 \u043f\u043e \u0444\u0438\u043d\u0430\u043d\u0441\u0430\u043c \u0417\u0410\u041e &laquo;\u0413\u043b\u043e\u0431\u0443\u0441&raquo;, \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a<\/p>\n<p>2004 \u0433\u043e\u0434- \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440\u0430 \u043f\u043e \u0444\u0438\u043d\u0430\u043d\u0441\u0430\u043c \u041e\u041e\u041e &laquo;\u0420\u0435\u0433\u0438\u043e\u043d-\u041a&raquo;, \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a<\/p>\n<p>2004-2005 \u0433\u043e\u0434\u044b - \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a \u043e\u0442\u0434\u0435\u043b\u0430 \u043b\u0435\u0441\u043e\u043f\u0440\u043e\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u043e\u0433\u043e \u043a\u043e\u043c\u043f\u043b\u0435\u043a\u0441\u0430 \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u043b\u0435\u0441\u043d\u043e\u0433\u043e \u043a\u043e\u043c\u043f\u043b\u0435\u043a\u0441\u0430 \u0421\u043e\u0432\u0435\u0442\u0430 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>\n<p>2005 &ndash; 2007 \u0433\u043e\u0434\u044b - \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a \u043e\u0442\u0434\u0435\u043b\u0430 \u043b\u0435\u0441\u043d\u043e\u0439 \u043f\u0440\u043e\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u0430\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u0430 \u043f\u0440\u0438\u0440\u043e\u0434\u043e\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u044f \u0438 \u043b\u0435\u0441\u043d\u043e\u0439 \u043e\u0442\u0440\u0430\u0441\u043b\u0438 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>\n<p>2007- 2008 \u0433\u043e\u0434\u044b - \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a \u043e\u0442\u0434\u0435\u043b\u0430 \u043b\u0435\u0441\u043d\u043e\u0439 \u043f\u0440\u043e\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u0438 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u044d\u043a\u0441\u043f\u0435\u0440\u0442\u0438\u0437\u044b \u043f\u0440\u043e\u0435\u043a\u0442\u043e\u0432 \u043e\u0441\u0432\u043e\u0435\u043d\u0438\u044f \u043b\u0435\u0441\u043e\u0432 \u0430\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u0430 \u043b\u0435\u0441\u043d\u043e\u0439 \u043e\u0442\u0440\u0430\u0441\u043b\u0438 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>\n<p>2008 \u0433\u043e\u0434 - \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044f \u0430\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u0430 &ndash; \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a \u043e\u0442\u0434\u0435\u043b\u0430 \u043b\u0435\u0441\u043d\u043e\u0439 \u043f\u0440\u043e\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u0438 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u044d\u043a\u0441\u043f\u0435\u0440\u0442\u0438\u0437\u044b \u043f\u0440\u043e\u0435\u043a\u0442\u043e\u0432 \u043e\u0441\u0432\u043e\u0435\u043d\u0438\u044f \u043b\u0435\u0441\u043e\u0432 \u0430\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u0430 \u043b\u0435\u0441\u043d\u043e\u0439 \u043e\u0442\u0440\u0430\u0441\u043b\u0438 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>\n<p>2008 \u0433\u043e\u0434 - \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044f \u0430\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u0430 \u043b\u0435\u0441\u043d\u043e\u0439 \u043e\u0442\u0440\u0430\u0441\u043b\u0438 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>\n<p>2008 &ndash; 2009 \u0433\u043e\u0434\u044b - \u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e \u0437\u0430\u043c\u0435\u0449\u0430\u044e\u0449\u0430\u044f \u0434\u043e\u043b\u0436\u043d\u043e\u0441\u0442\u044c \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044f \u0430\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u0430 \u043b\u0435\u0441\u043d\u043e\u0439 \u043e\u0442\u0440\u0430\u0441\u043b\u0438 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>\n<p>2009 &ndash; 2010 \u0433\u043e\u0434\u044b - \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430 \u044d\u043a\u043e\u043d\u043e\u043c\u0438\u043a\u0438 \u0438 \u0440\u0435\u0433\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u044f \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>\n<p>2010 &ndash; 2011 \u0433\u043e\u0434\u044b - \u043f\u0435\u0440\u0432\u044b\u0439 \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u0433\u0435\u043d\u0435\u0440\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440\u0430, \u0438\u0441\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440 \u041e\u041e\u041e &laquo;\u041a\u0440\u0430\u0435\u0432\u0430\u044f \u044d\u043d\u0435\u0440\u0433\u043e\u0441\u0431\u0435\u0440\u0435\u0433\u0430\u044e\u0449\u0430\u044f \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u044f&raquo;<\/p>\n<p>2011 \u0433\u043e\u0434 - \u0433\u0435\u043d\u0435\u0440\u0430\u043b\u044c\u043d\u044b\u0439 \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440 \u041e\u0410\u041e &laquo;\u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0438\u0439 \u0442\u0435\u0445\u043d\u043e\u043f\u0430\u0440\u043a&raquo;<\/p>\n<p>2011 &ndash; 2013 \u0433\u043e\u0434\u044b - \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430 \u0438\u043d\u0432\u0435\u0441\u0442\u0438\u0446\u0438\u0439 \u0438 \u0438\u043d\u043d\u043e\u0432\u0430\u0446\u0438\u0439 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>\n<p>\u0421 \u043c\u0430\u0440\u0442\u0430 2013 \u0433\u043e\u0434\u0430 - \u043c\u0438\u043d\u0438\u0441\u0442\u0440 \u0438\u043d\u0432\u0435\u0441\u0442\u0438\u0446\u0438\u0439 \u0438 \u0438\u043d\u043d\u043e\u0432\u0430\u0446\u0438\u0439 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>","person_bio_short":"<p>\u041c\u0438\u043d\u0438\u0441\u0442\u0440 \u0438\u043d\u0432\u0435\u0441\u0442\u0438\u0446\u0438\u0439 \u0438 \u0438\u043d\u043d\u043e\u0432\u0430\u0446\u0438\u0439 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>","person_foto":"","person_clean_url":"ruxullaeva-olga-vladimirovna","person_order":"0"}, {"person_id":"93","person_name":"\u041d\u0438\u043a\u043e\u043b\u0430\u0439 \u0421\u0435\u0440\u0433\u0435\u0435\u0432\u0438\u0447","person_last_name":"\u0413\u043b\u0443\u0448\u043a\u043e\u0432","person_status":"\u0414\u0435\u043f\u0443\u0442\u0430\u0442 \u0417\u0421 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f, \u0431\u044b\u0432\u0448\u0438\u0439 \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u0433\u0443\u0431\u0435\u0440\u043d\u0430\u0442\u043e\u0440\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f","person_bio":"<p><strong>\u0414\u0430\u0442\u0430 \u0440\u043e\u0436\u0434\u0435\u043d\u0438\u044f:<\/strong> 16 \u0438\u044e\u043b\u044f 1955 \u0433.<\/p>\n<p><strong>\u0422\u0440\u0443\u0434\u043e\u0432\u0430\u044f \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c:<\/strong><\/p>\n<p>1977&nbsp;\u0433. -&nbsp;\u043e\u043a\u043e\u043d\u0447\u0438\u043b \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0438\u0439 \u043f\u043e\u043b\u0438\u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442, \u043f\u043e\u043b\u0443\u0447\u0438\u043b \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u044c &laquo;\u0438\u043d\u0436\u0435\u043d\u0435\u0440-\u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c&raquo;.<\/p>\n<p>1977&ndash;1985 \u0433\u0433. -&nbsp;\u0440\u0430\u0431\u043e\u0442\u0430\u043b \u043d\u0430&nbsp;\u0414\u043e\u043c\u043e\u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u043c \u043a\u043e\u043c\u0431\u0438\u043d\u0430\u0442\u0435 \u2116&nbsp;2, \u0441\u0442\u0430\u043b \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a\u043e\u043c \u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u2116&nbsp;84.<\/p>\n<p>1985&ndash;1990 \u0433\u0433. -&nbsp;\u0440\u0430\u0431\u043e\u0442\u0430\u043b \u0432&nbsp;\u0433\u043e\u0440\u043a\u043e\u043c\u0435 \u041a\u041f\u0421\u0421 \u0438&nbsp;\u043a\u0440\u0430\u0439\u043a\u043e\u043c\u0435 \u041a\u041f\u0421\u0421 \u0438\u043d\u0441\u0442\u0440\u0443\u043a\u0442\u043e\u0440\u043e\u043c \u043e\u0442\u0434\u0435\u043b\u0430 \u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430, \u0437\u0430\u0432\u0435\u0434\u0443\u044e\u0449\u0438\u043c \u043e\u0442\u0434\u0435\u043b\u043e\u043c \u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u0438&nbsp;\u0433\u043e\u0440\u043e\u0434\u0441\u043a\u043e\u0433\u043e \u0445\u043e\u0437\u044f\u0439\u0441\u0442\u0432\u0430.<\/p>\n<p>1990&ndash;1992 \u0433\u0433. -&nbsp;\u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u043f\u0440\u0435\u0434\u0441\u0435\u0434\u0430\u0442\u0435\u043b\u044f \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u0433\u043e\u0440\u0438\u0441\u043f\u043e\u043b\u043a\u043e\u043c\u0430 \u043f\u043e&nbsp;\u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0443.<\/p>\n<p>1992&ndash;1993 \u0433\u0433. -&nbsp;\u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440 \u0434\u0435\u043f\u0430\u0440\u0442\u0430\u043c\u0435\u043d\u0442\u0430 \u043f\u043e&nbsp;\u0430\u0440\u0445\u0438\u0442\u0435\u043a\u0442\u0443\u0440\u0435 \u0438&nbsp;\u0433\u0440\u0430\u0434\u043e\u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0443 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0430.<\/p>\n<p>1993&ndash;1996 \u0433\u0433. -&nbsp;\u043f\u0440\u0435\u0434\u0441\u0435\u0434\u0430\u0442\u0435\u043b\u044c \u043a\u043e\u043c\u0438\u0442\u0435\u0442\u0430 \u043f\u043e&nbsp;\u0430\u0440\u0445\u0438\u0442\u0435\u043a\u0442\u0443\u0440\u0435 \u0438&nbsp;\u0433\u0440\u0430\u0434\u043e\u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0443 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f, \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u0433\u043b\u0430\u0432\u044b \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u043a\u0440\u0430\u044f.<\/p>\n<p>1996&ndash;1998 \u0433\u0433. -&nbsp;\u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u0433\u0443\u0431\u0435\u0440\u043d\u0430\u0442\u043e\u0440\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f \u043f\u043e&nbsp;\u0441\u043e\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u0441\u0444\u0435\u0440\u0435.<\/p>\n<p>\u0421&nbsp;1998&nbsp;\u0433. -&nbsp;\u043f\u0435\u0440\u0432\u044b\u0439 \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u0433\u0435\u043d\u0435\u0440\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440\u0430 \u0417\u0410\u041e &laquo;\u0421\u043b\u0430\u0432\u043d\u0435\u0444\u0442\u044c&nbsp;- \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043d\u0435\u0444\u0442\u0435\u0433\u0430\u0437&raquo;.<\/p>\n<p>\u0412&nbsp;2000&nbsp;\u0433. \u0438\u0437\u0431\u0440\u0430\u043d \u0434\u0435\u043f\u0443\u0442\u0430\u0442\u043e\u043c \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u0433\u043e\u0440\u043e\u0434\u0441\u043a\u043e\u0433\u043e \u0441\u043e\u0432\u0435\u0442\u0430 \u043f\u043e&nbsp;\u0441\u0435\u0434\u044c\u043c\u043e\u043c\u0443 \u043e\u0434\u043d\u043e\u043c\u0430\u043d\u0434\u0430\u0442\u043d\u043e\u043c\u0443 \u0438\u0437\u0431\u0438\u0440\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u043c\u0443 \u043e\u043a\u0440\u0443\u0433\u0443 \u0421\u043e\u0432\u0435\u0442\u0441\u043a\u043e\u0433\u043e \u0440\u0430\u0439\u043e\u043d\u0430.<\/p>\n<p>8&nbsp;\u043e\u043a\u0442\u044f\u0431\u0440\u044f 2002&nbsp;\u0433. \u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d \u043d\u0430&nbsp;\u0434\u043e\u043b\u0436\u043d\u043e\u0441\u0442\u044c \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044f \u0433\u0443\u0431\u0435\u0440\u043d\u0430\u0442\u043e\u0440\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f \u043f\u043e&nbsp;\u043f\u0440\u043e\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u043e\u043c\u0443, \u0433\u0440\u0430\u0436\u0434\u0430\u043d\u0441\u043a\u043e\u043c\u0443 \u0438&nbsp;\u0434\u043e\u0440\u043e\u0436\u043d\u043e\u043c\u0443 \u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0443.<\/p>\n<p>\u0421&nbsp;30&nbsp;\u044f\u043d\u0432\u0430\u0440\u044f 2004&nbsp;\u0433. \u043a\u0443\u0440\u0438\u0440\u0443\u0435\u0442 \u0441\u0444\u0435\u0440\u0443 \u0436\u0438\u043b\u0438\u0449\u043d\u043e-\u043a\u043e\u043c\u043c\u0443\u043d\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0445\u043e\u0437\u044f\u0439\u0441\u0442\u0432\u0430.<\/p>\n<p>\u0421&nbsp;15&nbsp;\u0438\u044e\u043b\u044f 2008&nbsp;\u0433. -&nbsp;\u043c\u0438\u043d\u0438\u0441\u0442\u0440 \u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u0438&nbsp;\u0430\u0440\u0445\u0438\u0442\u0435\u043a\u0442\u0443\u0440\u044b \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f.<\/p>\n<p>12&nbsp;\u0430\u043f\u0440\u0435\u043b\u044f 2010 \u0433\u043e\u0434\u0430 \u0433\u0443\u0431\u0435\u0440\u043d\u0430\u0442\u043e\u0440\u043e\u043c \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f \u041b\u044c\u0432\u043e\u043c \u041a\u0443\u0437\u043d\u0435\u0446\u043e\u0432\u044b\u043c \u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d \u043d\u0430&nbsp;\u043f\u043e\u0441\u0442 \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044f \u0433\u0443\u0431\u0435\u0440\u043d\u0430\u0442\u043e\u0440\u0430, \u043a\u043e\u043e\u0440\u0434\u0438\u043d\u0438\u0440\u0443\u044e\u0449\u0435\u0433\u043e \u0440\u0430\u0431\u043e\u0442\u0443 \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u0432\u043d\u0435\u0448\u043d\u0438\u0445 \u0441\u0432\u044f\u0437\u0435\u0439 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u0433\u043b\u0430\u0432\u044b \u0440\u0435\u0433\u0438\u043e\u043d\u0430, \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e\u0433\u043e \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f \u043f\u0440\u0438 \u043f\u0440\u0430\u0432\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0435 \u0420\u043e\u0441\u0441\u0438\u0439\u0441\u043a\u043e\u0439 \u0424\u0435\u0434\u0435\u0440\u0430\u0446\u0438\u0438, \u0430&nbsp;\u0442\u0430\u043a\u0436\u0435 \u0432\u0437\u0430\u0438\u043c\u043e\u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0435 \u0441&nbsp;\u0422\u0430\u0439\u043c\u044b\u0440\u0441\u043a\u0438\u043c \u0438&nbsp;\u042d\u0432\u0435\u043d\u043a\u0438\u0439\u0441\u043a\u0438\u043c \u043c\u0443\u043d\u0438\u0446\u0438\u043f\u0430\u043b\u044c\u043d\u044b\u043c\u0438 \u0440\u0430\u0439\u043e\u043d\u0430\u043c\u0438<\/p>\n<p>\u0412&nbsp;\u0434\u0435\u043a\u0430\u0431\u0440\u0435 2011 \u0433\u043e\u0434\u0430 \u0438\u0437\u0431\u0440\u0430\u043d \u0434\u0435\u043f\u0443\u0442\u0430\u0442\u043e\u043c \u0417\u0430\u043a\u043e\u043d\u043e\u0434\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u0421\u043e\u0431\u0440\u0430\u043d\u0438\u044f \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f<\/p>\n<p>\u0427\u043b\u0435\u043d \u043f\u0440\u0435\u0437\u0438\u0434\u0438\u0443\u043c\u0430 \u0440\u0435\u0433\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u043f\u043e\u043b\u0438\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u0441\u043e\u0432\u0435\u0442\u0430 \u043f\u0430\u0440\u0442\u0438\u0438 &laquo;\u0415\u0434\u0438\u043d\u0430\u044f \u0420\u043e\u0441\u0441\u0438\u044f&raquo;<\/p>","person_bio_short":"<p>\u0414\u0435\u043f\u0443\u0442\u0430\u0442 \u0417\u0421 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f, \u0431\u044b\u0432\u0448\u0438\u0439 \u0437\u0430\u043c\u0435\u0441\u0442\u0438\u0442\u0435\u043b\u044c \u0433\u0443\u0431\u0435\u0440\u043d\u0430\u0442\u043e\u0440\u0430 \u041a\u0440\u0